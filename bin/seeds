#!/usr/bin/env node
'use strict';

process.title = 'Seeds.js';

const resolve  = require('resolve');
const basepath = process.cwd();

resolve('seeds', {basedir: basepath}, function(err, localCLI) {
  let CLI;
  if (err) {
    CLI = require('../lib/cli');
  } else {
    CLI = require(localCLI);
  }
  let cli = new CLI(process.argv.slice(2));

  cli.command();
});
